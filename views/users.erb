<h3>Service desk members</h3>

<table class="box-table-a">
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>Wins</th>
    <th>Free bet count</th>
    <th>Last login</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.username %></td>
    <td><%=h user.first_name %> <%=h user.last_name %></td>
    <td><%=h user.bet_balance %></td>
    <td><%= user.p1_counts + user.p2_counts %></td>
    <td><%=h format_date(user.last_login_at) %></td>
  </tr>
<% end %>
</table>
<br />
